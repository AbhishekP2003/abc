module jkf1(clk, res, j, k, q, qb);
 input clk, res, j, k;
 output q, qb;
 reg q, qb;
 always @(posedge clk or posedge res) begin
 if (res) begin
 q <= 1'b0;
 qb <= 1'b1;
 end else begin
 if (j == 1'b0 && k == 1'b0) begin
 q <= q;
 qb <= qb;
 end else if (j == 1'b0 && k == 1'b1) begin
 q <= 1'b0;
 qb <= 1'b1;
 end else if (j == 1'b1 && k == 1'b0) begin
 q <= 1'b1;
 qb <= 1'b0;
 end else if (j == 1'b1 && k == 1'b1) begin
 q <= ~q;
 qb <= ~q;
 end
 end
 end
endmodule 
