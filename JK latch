`resetall
`timescale 1ns/1ns
module jklatch(en,j,k,q,qb);
input j,k,en;
output q, qb;
reg q, qb;
always @(en or j or k)
begin
if(!en)
begin
q=1'bz;
qb=1'bz;
end
else
begin
if(j==1'b0 && k==1'b0)
begin
q=q;
qb=qb;
end
else if(j==1'b0 && k==1'b1)
begin
q=1'b0;
qb=1'b1;
end
else if(j==1'b1 && k==1'b0)
begin
q=1'b1;
qb=1'b0;
end
else
begin
q=~q;
qb=~qb;
end
end
end
endmodule
